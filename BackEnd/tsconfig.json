{
  "compilerOptions": {
    "target": "es6",
    "module": "Node16",
    "rootDir": "./src",
    "moduleResolution": "node16",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true ,

    "strict": true ,
    "skipLibCheck": true ,
    "baseUrl": "src",
    "paths": {
      "@Controllers/*": ["controllers/*"],
      "@Validations/*": ["validations/*"],
      "@Services/*": ["services/*"],
      "@Routes/*":["routes/*"],
      "@Dictionary":["dictionary/index.ts"],
      "@Models/*": ["database/models/*"],
      "@Database/*":["database/*"]
    }
  }
}
